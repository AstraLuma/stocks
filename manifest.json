{
	"manifest_version": 2,
	"name": "Stock Calendar",
	"short_name": "Stocks",
	"description": "Tracks Stocks",
	"version": "0.4",
	"browser_action": {
		"default_icon": {
			"19": "images/icon-16.png",
			"38": "images/icon-32.png"
		},
		"default_title": "Stocks Calendar"
	},
	"icons": { 
		"16": "images/icon-16.png",
		"24": "images/icon-24.png",
		"32": "images/icon-32.png",
		"48": "images/icon-48.png",
		"64": "images/icon-64.png",
		"128": "images/icon-128.png",
		"256": "images/icon-256.png"
	},
	"permissions": [
		"http://insights.themarketiq.com/*",
		"http://online.wsj.com/*",
		"http://download.finance.yahoo.com/*"
	],
	"background": {
		"scripts": ["javascripts/event-page.js"],
		"persistent": false
	},
	"offline_enabled": false
}